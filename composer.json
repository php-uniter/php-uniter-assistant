{
    "name": "php-uniter/php-uniter-assistant",
    "homepage": "https://github.com/php-uniter/php-uniter-assistant",
    "license": "MIT",
    "type": "library",
    "description": "",
    "keywords": [
        "php-uniter",
        "php-uniter-assistant"
    ],
    "autoload": {
        "psr-4": {
            "PhpUniter\\PhpUniterPackage\\": "src/",
            "PhpUniter\\PhpUniterPackage\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Eduard Fomin",
            "email": "eduard.fomin@uniter1.tech"
        }
    ],
    "require": {
        "php": "^7.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "v3.*",
        "phpunit/phpunit": "9.*",
        "vimeo/psalm": "4.9.2"
    },
    "scripts": {
        "php-cs-fixer": "php ./vendor/bin/php-cs-fixer  fix --config=.php_cs.dist ./",
        "psalm": "php ./vendor/bin/psalm -c psalm.xml",
        "test": "php ./vendor/bin/phpunit"
    }
}
